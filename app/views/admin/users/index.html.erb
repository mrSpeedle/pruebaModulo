<h1>Usuarios</h1>
<%= link_to 'Nuevo Usuario', new_admin_user_path %>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Rol</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td>
          <%= link_to 'Editar', edit_admin_user_path(user) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>